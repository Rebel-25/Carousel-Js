{
    "env": {
        "browser": true,
        "es2021": true
 
    },
    "extends": [
        "eslint:recommended"
        //    "eslint:airbnb/base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"]
    }

    
}

